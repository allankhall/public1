package com.pacobass.amfobject
{
	[RemoteClass(alias = "pictureData2")]
	public class pictureData2
	{
		private var _fileName:String; 
		private var _picID:int;
		private var _fullPath:String;
		private var _instrumentID:int;
		private var _serial:String;
		public function pictureData2()
		{
		}
		[Bindable]
		public function get serial():String
		{
			return _serial;
		}

		public function set serial(value:String):void
		{
			_serial = value;
		}

		public function get instrumentID():int
		{
			return _instrumentID;
		}

		public function set instrumentID(value:int):void
		{
			_instrumentID = value;
		}

		[Bindable]
		public function get fullPath():String
		{
			return _fullPath;
		}
		
		public function set fullPath(value:String):void
		{
			_fullPath = value;
		}
		[Bindable]
		public function get picID():int
		{
			return _picID;
		}
		
		public function set picID(value:int):void
		{
			_picID = value;
		}
	
		[Bindable]
		public function get fileName():String
		{
			return _fileName;
		}
		
		public function set fileName(value:String):void
		{
			_fileName = value;
		}
		

	}
}