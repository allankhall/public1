package com.pacobass.amfobject{
	
	[RemoteClass(alias = "pictureData")]
	import flash.utils.ByteArray;
	
	public class pictureData{
		private var _fileName:String; 
		private var _fileData:ByteArray;
		private var _picID:int;
		private var _fullPath:String;
		public function pictureData(){
		}
[Bindable]
		public function get fullPath():String
		{
			return _fullPath;
		}

		public function set fullPath(value:String):void
		{
			_fullPath = value;
		}
[Bindable]
		public function get picID():int
		{
			return _picID;
		}

		public function set picID(value:int):void
		{
			_picID = value;
		}
[Bindable]
 	public function get fileData():ByteArray
		{
			return _fileData;
		}

		public function set fileData(value:ByteArray):void
		{
			_fileData = value;
		}
[Bindable]
		public function get fileName():String
		{
			return _fileName;
		}

		public function set fileName(value:String):void
		{
			_fileName = value;
		}

	}


}