package com.pacobass.amfobject
{
	[RemoteClass(alias = "userData")]
	public class userData
	{
		private var _userID:String;
		private var _username:String;
		private var _firstName:String;
		private var _lastName:String;
		private var _email:String;
		private var _status:String;
		private var _role:String;
		private var _password:String;
		
		[Bindable]
		public function set userID(userID:String):void
		{
			_userID = userID;
		}
		public function get userID():String{
			return _userID;
		}
		
		[Bindable]
		public function set username(username:String):void
		{
			_username = username;
		}
		public function get username():String{
			return _username;
		}
		
		[Bindable]
		public function set firstName(firstName:String):void
		{
			_firstName = firstName;
		}
		public function get firstName():String{
			return _firstName;	
		}
		
		[Bindable]
		public function set lastName(lastName:String):void
		{
			_lastName = lastName;
		}
		public function get lastName():String{
			return _lastName;
		}
		
		[Bindable]
		public function set email(email:String):void
		{
			_email = email;
		}
		public function get email():String{
			return _email;
		}
		
		[Bindable]
		public function set status(status:String):void
		{
			_status = status;
		}
		public function get status():String
		{
			return _status;
		}
		
		[Bindable]
		public function set role(role:String):void
		{
			_role = role;
		}
		public function get role():String
		{
			return _role;
		}
		
		[Bindable]
		public function set password(password:String):void
		{
			_password = password;
		}
		public function get password():String
		{
			return _password;
		}
	}
}