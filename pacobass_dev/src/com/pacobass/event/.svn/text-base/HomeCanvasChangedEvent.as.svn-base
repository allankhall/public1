package com.pacobass.event
{
	import flash.events.Event;

	public class HomeCanvasChangedEvent extends Event
	{
		/**
		 * pass on when a canvas is shown in home box
		 */
		public static const CANVAS_CHANGED:String = "homeCanvasChanged";
		public var selectedCanvas:String;
		public function HomeCanvasChangedEvent(selectedCanvas:String)
		{
			super(CANVAS_CHANGED);
			this.selectedCanvas = selectedCanvas;
		}
		
	}
}