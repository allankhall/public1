package com.pacobass.event
{
	import flash.events.Event;

	public class InstrumentHelperEvent extends Event
	{
		/**
		 * pass button events to contributorbox from child components
		 * 
		 */
		public static const INSTRUMENT_HELPER:String = "instrumentHelper";
		public var button:String;
		public function InstrumentHelperEvent(button:String)
		{
			super(INSTRUMENT_HELPER);
			this.button = button;
		}
		
	}
}