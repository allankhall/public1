package com.pacobass.event.homecomponent
{
	import com.pacobass.amfobject.instrumentData;
	
	import flash.events.Event;
	

	
	public class InstrumentDGHelperEvent extends Event
	{
		
		public static const INSTRUMENT_DG_HELPER:String = "InstrumentDGHelperEvent";
		public var button:String;
		public var instrument:instrumentData; 
		
		public function InstrumentDGHelperEvent(button:String, instrument:instrumentData)
		{
			
			super(INSTRUMENT_DG_HELPER);
			this.button = button;
			this.instrument = instrument;
		}

	}
}